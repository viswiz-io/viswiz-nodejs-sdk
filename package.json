{
  "name": "viswiz-sdk",
  "description": "The VisWiz.io Node.js SDK",
  "version": "1.1.0",
  "main": "dist/sdk.js",
  "author": "Valentin Agachi <valentin@viswiz.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/viswiz-io/viswiz-nodejs-sdk"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.5.0"
  },
  "files": [
    "dist/sdk.js",
    "es.js",
    "index.js"
  ],
  "scripts": {
    "build": "babel src/ --out-dir dist/",
    "commitmsg": "validate-commit-msg",
    "docs": "esdoc",
    "lint": "eslint . --cache --fix --ignore-pattern '!.eslintrc.js'",
    "pretty": "prettier --write --list-different \"**/*.js\"",
    "precommit": "lint-staged",
    "prepack": "npm run build",
    "release": "standard-version -a",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest",
    "pretest": "npm run lint"
  },
  "dependencies": {
    "form-data": "^2.3.1",
    "got": "^8.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "esdoc": "^1.0.4",
    "esdoc-brand-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.12.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "nock": "^9.1.4",
    "prettier": "^1.8.2",
    "standard-version": "^4.2.0",
    "validate-commit-msg": "^2.14.0"
  }
}
